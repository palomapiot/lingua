{
  "name": "lingua",
  "version": "0.0.0",
  "private": true,
  "description": "Description for Lingua",
  "license": "UNLICENSED",
  "scripts": {
    "build": "npm run webapp:prod --",
    "build-watch": "concurrently 'npm run webapp:build:dev -- --watch'",
    "ci:frontend:build": "npm run webapp:build:$npm_package_config_default_environment",
    "ci:frontend:test": "npm run ci:frontend:build && npm test",
    "clean-www": "rimraf target/classes/static/",
    "cleanup": "rimraf target/",
    "lint": "eslint . --ext .ts,.vue ",
    "lint:fix": "eslint . --ext .ts,.vue --fix",
    "prepare": "husky",
    "prettier:check": "prettier --check \"{,src/**/,.blueprint/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,vue}\"",
    "prettier:format": "prettier --write \"{,src/**/,.blueprint/**/}*.{md,json,yml,html,cjs,mjs,js,ts,tsx,css,scss,vue}\"",
    "serve": "npm run start --",
    "start": "npm run webapp:dev --",
    "start-tls": "npm run webapp:dev -- --env.tls",
    "pretest": "npm run lint",
    "test": "npm run vitest-run --",
    "test:watch": "npm run vitest",
    "vite-build": "vite build",
    "vite-serve": "vite",
    "vitest": "vitest",
    "vitest-run": "vitest --run --coverage",
    "watch": "concurrently npm:start",
    "webapp:build": "npm run clean-www && npm run webapp:build:dev --",
    "webapp:build:dev": "npm run vite-build",
    "webapp:build:prod": "npm run vite-build",
    "webapp:dev": "npm run vite-serve",
    "webapp:prod": "npm run clean-www && npm run webapp:build:prod --",
    "webapp:serve": "npm run vite-serve",
    "webapp:test": "npm run test --"
  },
  "config": {
    "default_environment": "prod"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "6.5.2",
    "@fortawesome/free-solid-svg-icons": "6.5.2",
    "@fortawesome/vue-fontawesome": "3.0.6",
    "@vue/compat": "3.4.21",
    "@vuelidate/core": "2.0.3",
    "@vuelidate/validators": "2.0.4",
    "@vueuse/core": "10.9.0",
    "axios": "1.6.8",
    "bootstrap": "4.6.2",
    "bootstrap-vue": "2.23.1",
    "bootswatch": "4.6.2",
    "dayjs": "1.11.11",
    "deepmerge": "4.3.1",
    "pinia": "2.1.7",
    "vue": "3.4.21",
    "vue-i18n": "9.13.1",
    "vue-router": "4.3.2"
  },
  "devDependencies": {
    "@pinia/testing": "0.1.3",
    "@rushstack/eslint-patch": "1.10.2",
    "@tsconfig/node18": "18.2.4",
    "@types/node": "20.11.25",
    "@types/sinon": "17.0.3",
    "@vitejs/plugin-vue": "5.0.4",
    "@vitest/coverage-v8": "1.5.2",
    "@vue/eslint-config-prettier": "9.0.0",
    "@vue/eslint-config-typescript": "10.0.0",
    "@vue/test-utils": "2.4.5",
    "@vue/tsconfig": "0.5.1",
    "autoprefixer": "10.4.19",
    "axios-mock-adapter": "1.22.0",
    "concurrently": "8.2.2",
    "eslint": "8.57.0",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-vue": "8.7.1",
    "flush-promises": "1.0.2",
    "generator-jhipster": "8.4.0",
    "happy-dom": "14.7.1",
    "husky": "9.0.11",
    "lint-staged": "15.2.2",
    "numeral": "2.0.6",
    "postcss-import": "16.1.0",
    "postcss-url": "10.1.3",
    "prettier": "3.2.5",
    "prettier-plugin-java": "2.6.0",
    "prettier-plugin-packagejson": "2.5.0",
    "rimraf": "5.0.5",
    "sass": "1.75.0",
    "sinon": "17.0.1",
    "swagger-ui-dist": "5.17.2",
    "typescript": "5.4.5",
    "vite": "5.2.10",
    "vite-plugin-static-copy": "1.0.3",
    "vitest": "1.5.2",
    "vitest-sonar-reporter": "2.0.0",
    "wait-on": "7.2.0"
  },
  "engines": {
    "node": ">=20.12.2",
    "npm": ">= 6.14.4"
  },
  "cacheDirectories": [
    "node_modules"
  ]
}
